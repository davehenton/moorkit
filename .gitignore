# # Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# do not scheck in a schema.rb
/db/schema.rb

# Ignore all logfiles and tempfiles.
/log/*.log
log/*.age
/tmp

# ignore SimpleCov coverage files
coverage

# ignore byebug
.byebug_history

# ignore scratch files
scratch

# Ignore vcr output
spec/vcr

# Ignore application configuration
/config/sittercity_settings.yml

# # Ignore packaging artifacts
version.txt
vendor/bundle/*
*.deb
vendor_bundle

# Ignore custom environments
*.dev_alt
*.test_alt
.env
config.dev

# os specific files
.DS_Store

# emacs temp fils
*~
\#*\#
.\#*

# dist files
.public_assets
git.commit
git.revision
git.count
package/
public/assets

.sso-db-migrate
.seed_counter

resque.pid
resque_scheduler.pid
